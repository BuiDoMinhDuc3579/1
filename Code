TI = 'TypeI.gsheet'
TJ = 'TypeJ.gsheet'
search_path = '/content/drive/My Drive'

if FilmA.output['Loại phim'] == 'A':
  file_path = None
  for root, dirs, files in os.walk(search_path):
      if TA in files:
         file_path = os.path.join(root, TA)
         break
  df = pd.read_excel(file_path)

