import numpy as np
import skfuzzy as fuzz 
from skfuzzy import control as ctrl 
import pandas as pd 

Mood = ctrl.Antecedent(np.arange(0,11.1,1),'Tâm trạng')
Age = ctrl.Antecedent(np.arange(0,101,1),'Tuổi')
Pace = ctrl.Antecedent(np.arange(0,11,0.1),'Nhịp độ phim')
Time = ctrl.Antecedent(np.arange(1990,2025,1),'Thời đại')
Length = ctrl.Antecedent(np.arange(10,181,1),'Độ dài phim')
Type = ctrl.Consequent(np.arange(0,11,1),'Loại phim')

Mood['Depressed'] = fuzz.trimf(Mood.universe,[0,0,2])
Mood['Sad'] = fuzz.trimf(Mood.universe,[2,4,6])
Mood['Bored'] = fuzz.trimf(Mood.universe,[4,6,8])
Mood['Happy'] = fuzz.trimf(Mood.universe,[6,8,10])
Mood['Romantic'] = fuzz.trimf(Mood.universe,[8,10,10])

Age['All'] = fuzz.trimf(Age.universe,[0,0,8])
Age['Children'] = fuzz.trimf(Age.universe,[0,8,12])
Age['Teenager'] = fuzz.trimf(Age.universe,[12,16,18])
Age['Adult'] = fuzz.trimf(Age.universe,[18,100,100])

Pace['Slow'] = fuzz.trimf(Pace.universe,[0,0,5])
Pace['Mid'] = fuzz.trimf(Pace.universe,[0,5,10])
Pace['Fast'] = fuzz.trimf(Pace.universe,[5,10,10])

Time['90s'] = fuzz.trimf(Time.universe,[1990,1990,2000])
Time['2000s'] = fuzz.trimf(Time.universe,[2000,2000,2010])
Time['2010s'] = fuzz.trimf(Time.universe,[2010,2010,2020])
Time['2020s'] = fuzz.trimf(Time.universe,[2020,2020,2024])

Length['Short'] = fuzz.trimf(Length.universe,[10,10,20])
Length['Avg'] = fuzz.trimf(Length.universe,[10,20,30])
Length['Long'] = fuzz.trimf(Length.universe,[20,30,60])
Length['AvgMovie'] = fuzz.trimf(Length.universe,[60,120,120])
Length['LongMovie'] = fuzz.trimf(Length.universe,[120,180,180])

Type['A'] = fuzz.trimf(Type.universe,[0,0,1])
Type['B'] = fuzz.trimf(Type.universe,[1,1,2])
Type['C'] = fuzz.trimf(Type.universe,[2,2,3])
Type['D'] = fuzz.trimf(Type.universe,[3,3,4])
Type['E'] = fuzz.trimf(Type.universe,[4,4,5])
Type['F'] = fuzz.trimf(Type.universe,[5,5,6])
Type['G'] = fuzz.trimf(Type.universe,[6,6,7])
Type['H'] = fuzz.trimf(Type.universe,[7,7,8])
Type['I'] = fuzz.trimf(Type.universe,[8,8,9])
Type['J'] = fuzz.trimf(Type.universe,[9,9,10])

rule1 = ctrl.Rule(Mood['Depressed']& Age['All']& Pace['Slow']& Time['90s']& Length['Short'], Type ['A'] )
rule2 = ctrl.Rule(Mood['Depressed']& Age['All']& Pace['Slow']& Time['90s']& Length['Avg'], Type ['A'] )
rule3 = ctrl.Rule(Mood['Depressed']& Age['All']& Pace['Slow']& Time['90s']& Length['Long'], Type ['A'] )
